<body>
    {% block header %}
        {% block nav %}
<body>
  <a href=""{{ path('app_index') }}"" class="btn bg-primary text-white position-fixed mt-3 ms-3" data-bs-toggle="tooltip"
     data-bs-placement="right" title="Ne pas inclure dans l'intégration">Retour</a>
  <header>
    <nav class="navbar navbar-expand-md bg-light navbar-light fixed-top">
      <div class="container">
        <a class="navbar-brand text-uppercase fw-bold" href=""{{ path('app_index') }}"">
          <span class="bg-primary bg-gradient p-1 rounded-3 text-light">S</span> F
        </a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <ul class="navbar-nav">
          <li class="nav-item active">
                <a class="nav-link" href="{{ path('app_index') }}">Accueil <span class="sr-only">(current)</span></a>
          </li>
          <li class="nav-item">
                <a class="nav-link" href="{{ path('app_categorie') }}"> {{ render(controller('App\\Controller\\CategorieController::list')) }}  </a>
          </li>

       
          <li class="nav-item">
      
            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
    
            {% if  is_granted('ROLE_ADMIN') %}
                <a class="nav-link" tabindex="-1" aria-disabled="true" href="{{ path('app_admin_logout') }}">Déconnectez-vous</a>
            {% else %}
                <a class="nav-link" tabindex="-1" aria-disabled="true" href="{{ path('app_logout') }}">Déconnectez-vous</a>
          </li>     
          <li class="nav-item">  
            {% if app.user %}
                <a class="nav-link" tabindex="-1" aria-disabled="true" href="{{ path('app_user') }}"> User : {{ app.user.userIdentifier }}</a>
            {% endif %}
            {% endif %}
            {% endif %}
           </li> 
           <li class="nav-item">
            {% if not is_granted('IS_AUTHENTICATED_FULLY') %}
                <a class="nav-link" tabindex="-1" aria-disabled="true" href="{{ path('app_login') }}">Connectez-vous</a>
            {% endif %}
           </li>

            {% endblock %}
            {% endblock %}

      </ul>
      </div>
    </nav>
  </header>

<header>    
   
<nav class="navbar navbar-expand-lg navbar-light bg-light">
    {# <nav> #}

    <div class="logo"> 
        <a class="navbar-brand" href="{{ path('app_index') }}"></a>
    </div>

    <button class="navbar-toggler" class="btn active" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
        
            {# <a class="navbar-toggler-icon" href="{{ path('app_index') }}">Accueil <span class="sr-only">(current)</span></a>
